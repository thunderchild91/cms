    position: relative;
    font-family: 'Lato', sans-serif;
    font-size: 14px;
    font-weight: 300;
    color: #E6E6FA;
    line-height: 1.6;
    text-transform: none;
    background-color: #7552BD;
}

footer {
    background: #6B37C1;
    padding: 40px 0 20px;
}
.lead.text-muted {
	color: #E6E6FA;
	font-size: 16px;
}
.lead.text-muted a{
    font-size: 14px;
}
h2 {
	font-size: 25px;
}
.navbar-default .navbar-nav>li>a {
    color: #E6E6FA;
}
.navbar-brand {
    color: #E6E6FA !important; 
}
.navbar-default .navbar-nav>li>a:hover, .navbar-default .navbar-nav>li>a:focus {
    color: #E6E6FA !important;
    background-color: #9d87cb;
    border-color: #482f7a;
}
.list-group-item {
    background: #7552bd;
    border-bottom: 2px solid #52368d;
    border-left: 2px solid #52368d;
    border-right: 2px solid #52368d;
    border-top: 2px solid #52368d;
    color: #E6E6FA !important;
}
a.list-group-item:hover, button.list-group-item:hover, a.list-group-item:focus, button.list-group-item:focus {
    text-decoration: none;
    color: #555;
    background-color: #9d87cb;
}
.list-group-item.active, .list-group-item.active:hover, .list-group-item.active:focus {
    z-index: 2;
    color: #E6E6FA;
    background-color: #5e3d9f;
    border-color: #482f7a;
}
.pagination>li>a, .pagination>li>span {
    position: relative;
    float: left;
    padding: 6px 12px;
    line-height: 1.42857143;
    text-decoration: none;
    color: #E6E6FA;
    background-color: #7552bd;
    border: 1px solid #52368d;
    margin-left: -1px;
}
.pagination>.disabled>span, .pagination>.disabled>span:hover, .pagination>.disabled>span:focus, .pagination>.disabled>a, .pagination>.disabled>a:hover, .pagination>.disabled>a:focus {
    color: #E6E6FA;
    background-color: #7552bd;
    border-color: #52368d;
    cursor: not-allowed;
}
.pagination>li>a:hover, .pagination>li>span:hover, .pagination>li>a:focus, .pagination>li>span:focus {
    z-index: 3;
    color: #E6E6FA;
    background-color: #9d87cb;
    border-color: #482f7a;
}
.pagination>.active>a, .pagination>.active>span, .pagination>.active>a:hover, .pagination>.active>span:hover, .pagination>.active>a:focus, .pagination>.active>span:focus {
    z-index: 2;
    color: #fff;
    background-color: #5e3d9f;
    border-color: #482f7a;
    cursor: default;
}
